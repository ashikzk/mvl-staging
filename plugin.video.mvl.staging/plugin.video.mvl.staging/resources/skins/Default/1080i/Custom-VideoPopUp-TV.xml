			</animation>
			<control type="image" description="Menu BG">
				<posx>595</posx>
				<posy>300</posy>
				<height>570</height>
				<width>700</width>
				<texture border="0,40,0,40">dialogs/ContextMenu.png</texture>
				<!--<texture border="0,40,0,40">dialogs/ContextMenu2.png</texture>-->
			</control>
		</control>

